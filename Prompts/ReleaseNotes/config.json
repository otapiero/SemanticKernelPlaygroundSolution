{
  "schema": 1,
  "type": "completion",
  "description": "Generates release notes based on git commits",
  "completion": {
    "max_tokens": 1000,
    "temperature": 0.7
  },
  "input": {
    "parameters": [
      {
        "name": "commitInfo",
        "description": "The commit information to summarize",
        "defaultValue": ""
      },
      {
        "name": "version",
        "description": "The version for the release",
        "defaultValue": "1.0.0"
      },
      {
        "name": "date",
        "description": "The date of the release note",
        "defaultValue": ""
      }
    ]
  }
}